SELECT * FROM movies;

SELECT name, genre, year
FROM movies;

SELECT name AS 'BALIONELIAI'
FROM movies;
SELECT imdb_rating AS 'IMDb'
FROM movies;
SELECT name AS 'BALIONELIAI', imdb_rating AS 'IMDb'
FROM movies;

SELECT DISTINCT year
FROM movies;

SELECT * 
FROM movies 
WHERE imdb_rating < 5;
SELECT * 
FROM movies 
WHERE year > 2014;

SELECT * 
FROM movies
WHERE name LIKE 'Se_en';

SELECT * 
FROM movies
WHERE name LIKE '%man%';
SELECT * 
FROM movies
WHERE name LIKE 'The%';

